<svg width="600" height="200" xmlns="http://www.w3.org/2000/svg" style="background-color:#1F2937; color: #9CA3AF;">
    <defs>
        <marker id="arrow" viewBox="0 0 10 10" refX="5" refY="5"
            markerWidth="6" markerHeight="6"
            orient="auto-start-reverse">
          <path d="M 0 0 L 10 5 L 0 10 z" fill="#60A5FA" />
        </marker>
    </defs>
    <style>
        .box { fill: #374151; stroke: #4B5563; stroke-width: 1; }
        .text { font-family: sans-serif; font-size: 16px; fill: #D1D5DB; text-anchor: middle; }
        .line { stroke: #60A5FA; stroke-width: 2; marker-end: url(#arrow); }
    </style>
    
    <!-- Boxes -->
    <rect x="50" y="80" width="150" height="40" rx="8" class="box" />
    <text x="125" y="105" class="text">User Input</text>
    
    <rect x="250" y="80" width="150" height="40" rx="8" class="box" />
    <text x="325" y="105" class="text">Server</text>
    
    <rect x="450" y="80" width="150" height="40" rx="8" class="box" />
    <text x="525" y="105" class="text">Database</text>
    
    <!-- Lines -->
    <line x1="200" y1="100" x2="250" y2="100" class="line" />
    <line x1="400" y1="100" x2="450" y2="100" class="line" />

    <text x="325" y="40" class="text" style="font-size:20px; font-weight: bold; fill: #F9FAFB;">User Login Flow</text>
</svg> 